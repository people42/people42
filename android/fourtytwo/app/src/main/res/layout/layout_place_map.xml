<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/place_frame"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="300dp">
    <fragment
        android:id="@+id/place_map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
            />
    <View
        android:layout_gravity="center"
        android:layout_width="@dimen/circle_shape"
        android:layout_height="@dimen/circle_shape"
        android:background="@drawable/circle_shape"
        />
    <View
        android:layout_gravity="center"
        android:layout_width="@dimen/circle_shape2"
        android:layout_height="@dimen/circle_shape2"
        android:background="@drawable/circle_shape"
        />
    <View
        android:layout_gravity="center"
        android:layout_width="@dimen/circle_shape3"
        android:layout_height="@dimen/circle_shape3"
        android:background="@drawable/circle_shape"
        />
    <View
        android:layout_gravity="center"
        android:layout_width="121dp"
        android:layout_height="121dp"
        android:background="@drawable/radar_circle"
        />
    <View
        android:layout_gravity="center"
        android:layout_width="298dp"
        android:layout_height="298dp"
        android:background="@drawable/radar_circle"
        />
    <View
        android:layout_gravity="center"
        android:layout_width="476dp"
        android:layout_height="476dp"
        android:background="@drawable/radar_circle3"
        />
    <ImageView
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:src="@drawable/center_location"
        android:layout_gravity="center"/>
</FrameLayout>
